<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="450" d:DesignHeight="450"
        x:Class="AgentsSecond.addingNewProduct"
        Title="addingNewProduct">
	<DockPanel>
		<StackPanel DockPanel.Dock="Top">
			<StackPanel>
				<TextBlock Text="Выберите название агента:"/>
				<ComboBox x:Name="AgentsCombobox"/>
			</StackPanel>

			<StackPanel>
				<TextBlock Text="Выберите навзвание товара:"/>
				<ComboBox x:Name="ProductCombobox"/>
			</StackPanel>

			<StackPanel>
				<TextBlock Text="Введите дату:"/>
				<TextBox x:Name="DataBox"/>
			</StackPanel>

			<StackPanel>
				<TextBlock Text="Введите количество товаров:"/>
				<TextBox x:Name="CountBox"/>
			</StackPanel>
			
			<Button Content="Добавить" Click="AddProductClick"/>
		</StackPanel>
		
	</DockPanel>
</Window>
